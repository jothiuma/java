package nandhini;
import java.util.Scanner;
public class count
{
	public static void main(String[]args) 
	{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter a value:");
	int w=s.nextInt();
	int count,c=0;
	for(int i=1;i<=w;i++)
	{
		count=0;
		 for(int j=2;j<=i/2;j++)
		 {
			 if(i%j==0)
			 {
				 count++;
			 }
		 }
	if(count==0)
		{
		c++;
		}
		}
System.out.println(c);
}
}
